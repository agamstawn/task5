<div class="col-md-12">
  <legend>Sign Up</legend>
  <%= form_tag sessions_path do %>
    <%= label_tag :username %>
    <%= text_field_tag :username, (params[:username] unless params[:username].blank?) %>
    <br/>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br/>
    <%= submit_tag :submit , class:"btn btn-default"%>
  <% end %>
</div>